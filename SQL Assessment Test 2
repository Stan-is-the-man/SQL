3:
SELECT *
FROM cd.facilities
WHERE membercost != 0

4:
SELECT facid, name, membercost, monthlymaintenance
FROM cd.facilities
WHERE membercost != 0
AND membercost < (monthlymaintenance * 0.02)

5:
SELECT *
FROM cd.facilities
WHERE name LIKE '%Tennis%'

6:
SELECT *
FROM cd.facilities
WHERE facid IN (1,5)

7:
SELECT memid, surname, firstname, joindate 
FROM cd.members
WHERE joindate > '2012-08-31'




