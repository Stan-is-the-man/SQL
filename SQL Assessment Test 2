3:
SELECT *
FROM cd.facilities
WHERE membercost != 0;

4:
SELECT facid, name, membercost, monthlymaintenance
FROM cd.facilities
WHERE membercost != 0
AND membercost < (monthlymaintenance * 0.02);

5:
SELECT *
FROM cd.facilities
WHERE name LIKE '%Tennis%';

6:
SELECT *
FROM cd.facilities
WHERE facid IN (1,5);

7:
SELECT memid, surname, firstname, joindate 
FROM cd.members
WHERE joindate > '2012-08-31';

8:
SELECT DISTINCT surname
FROM cd.members
ORDER BY surname
LIMIT 10;

9:
SELECT joindate
FROM cd.members
ORDER BY joindate DESC
LIMIT 1

10:
SELECT COUNT(*)
FROM cd.facilities
WHERE guestcost >= 10

11:
SELECT facid, SUM(slots)
FROM cd.bookings
WHERE starttime BETWEEN '2012-09-01' AND '2012-10-01'
GROUP BY facid
ORDER BY sum




